1 VERSIONINFO
FILEVERSION ${OBS_VERSION_MAJOR},${OLS_VERSION_MINOR},${OLS_VERSION_PATCH},0
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904B0"
    BEGIN
      VALUE "CompanyName", "${OLS_COMPANY_NAME}"
      VALUE "FileDescription", "OBS Studio scripting module"
      VALUE "FileVersion", "${OLS_VERSION_CANONICAL}"
      VALUE "ProductName", "${OLS_PRODUCT_NAME}"
      VALUE "ProductVersion", "${OLS_VERSION_CANONICAL}"
      VALUE "Comments", "${OLS_COMMENTS}"
      VALUE "LegalCopyright", "${OLS_LEGAL_COPYRIGHT}"
      VALUE "InternalName", "ols-scripting"
      VALUE "OriginalFilename", "ols-scripting"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 0x04B0
  END
END
