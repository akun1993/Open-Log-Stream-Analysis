1 VERSIONINFO
FILEVERSION ${UI_VERSION_MAJOR},${UI_VERSION_MINOR},${UI_VERSION_PATCH},0
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904B0"
    BEGIN
      VALUE "CompanyName", "${OLS_COMPANY_NAME}"
      VALUE "FileDescription", "${MODULE_DESCRIPTION}"
      VALUE "FileVersion", "${UI_VERSION}"
      VALUE "ProductName", "${OLS_PRODUCT_NAME}"
      VALUE "ProductVersion", "${UI_VERSION}"
      VALUE "Comments", "${OLS_COMMENTS}"
      VALUE "LegalCopyright", "${OLS_LEGAL_COPYRIGHT}"
      VALUE "InternalName", "${PROJECT_NAME}"
      VALUE "OriginalFilename", "${PROJECT_NAME}"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 0x04B0
  END
END